
public class Q1 {
	public static void main(String[] args)
	{
		System.out.println("U   U  B B      C C");
		System.out.println("U   U  B   B  C");
		System.out.println("U   U  B B    C");
		System.out.println("U   U  B   B  C");
		System.out.println(" U U   B B      C C");
	}

}

public class Q2 {
	public static void main(String[] args)
	{
		int w = 7;
		int h = 9;
		int area = w * h;
		int perimeter = (2 * w) + (2 * h);
		
		System.out.println("the area of the rectangle is "
				+ area);
		System.out.println("the perimeter of the rectangle is "
				+ perimeter);
	}

}


public class Q3 {
	public static void main(String[] args)
	{
		
		double g = 9.81;
		double t = 12;
		double d = (g * (t * t)) / 2;
		System.out.println("The object has fallen " +
		d + " after 12 seconds.");

	}

}
